using System;
     class FibonacciSeries
    {
    static void Main(string[] args)
    {
        Console.Write("Enter the number:");
        int n = int.Parse(Console.ReadLine());
        int first = 0, second = 1, next;
        Console.WriteLine("FIbonacci series: ");
        for (int i = 0; i < n; i++) {
            if (i <= 1) 
                next = i;
            else {
                    next = first + second;
                    first = second;
                    second = next;
                }
                Console.Write(next + "");
            }
        }
    }

