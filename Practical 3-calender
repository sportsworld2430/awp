using System;
using System.Web.UI.WebControls;

namespace WebApplication6
{
    public partial class WebForm1 : System.Web.UI.Page
    {
        protected void Page_Load(object sender, EventArgs e)
        { 
        }
            protected void Calenndar1_DayRender(object sender, DayRenderEventArgs e)
        {
            if((e.Day.Date>=new DateTime(2024,8,14))&&(e.Day.Date<=new DateTime(2024, 8, 16)))
            {
                e.cell.BackColor=System.Drawing.Color.Red;
                e.cell.ForeColor=System.Drawing.Color.Green;
                e.cell.Controls.Add(new LiteralControl("Holiday"));
            }

        }

        protected void calender1_DayRender(object sender, DayRenderEventArgs e)
        {

        }
    }
    }
