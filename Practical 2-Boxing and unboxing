using System;
     class Program
    {
    static void Main(string[] args)
    {
        int a = 10;
        object obj = a;
        Console.WriteLine("a={0}",a);
        Console.WriteLine("obj={0}",obj);
        int b = (int)obj;
        Console.WriteLine("b={0}",b);
        Console.ReadKey();
        }
    }
